    "typescript": "^5.2.2",
    "typescript-cp": "^0.1.9"
  },
  "dependencies": {
    "@types/dotenv-parse-variables": "^2.0.2",
    "dotenv": "^16.3.1",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.0.0",
    "pino-http": "^8.5.0",
