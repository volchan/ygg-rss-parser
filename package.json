  "scripts": {
    "start": "nodemon -r tsconfig-paths/register -e ts,json,pug src/index.ts",
    "build": "rimraf ./dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && tscp",
    "serve": "node dist/index.js",
    "prepare": "pnpx husky install",
    "depcheck": "pnpx depcheck",
    "lint": "pnpm eslint && pnpm prettier",
    "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
    "eslint": "eslint --ext .js,.ts,.jsx,.tsx",
    "eslint:fix": "eslint --fix --ext .js,.ts,.jsx,.tsx",
    "prettier": "prettier --check './**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write './**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-pug": "^1.2.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
